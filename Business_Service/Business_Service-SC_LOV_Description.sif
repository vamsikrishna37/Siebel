function Service_PreInvokeMethod (MethodName, Inputs, Outputs)
{


  if(MethodName == &quot;GetLOVDescr&quot;){
	GetLOVDescr(Inputs, Outputs)
	 return (CancelOperation);  
    }
  return (ContinueOperation);
}
