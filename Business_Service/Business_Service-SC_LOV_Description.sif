			<BUSINESS_SERVICE_SERVER_SCRIPT
			   INACTIVE="N"
			   NAME="GetLOVDescr"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function GetLOVDescr(Inputs, Outputs){

	 var isRecord=&quot;&quot;,LOVdesc=&quot;&quot;,lsLOVBO,lsLOVBC;
     lsLOVBO = TheApplication().GetBusObject(&quot;List Of Values&quot;);
	 lsLOVBC = lsLOVBO.GetBusComp(&quot;List Of Values&quot;); 
	 var searchExpr = Inputs.GetProperty(&quot;searchExpr&quot;);
	 try{
		with(lsLOVBC) 
			{ 
				SetViewMode(AllView); 
				ClearToQuery(); 
				ActivateField(&quot;Description&quot;)
				SetSearchExpr(searchExpr);   
				ExecuteQuery(ForwardOnly);
				isRecord = FirstRecord();  
				if(isRecord) 
					{ 
						LOVdesc=GetFieldValue(&quot;Description&quot;);
					
					} 
			}
			Outputs.SetProperty(&quot;DESC&quot;,LOVdesc);
       }
	   catch(e){
	   TheApplication().RaiseErrorText(&quot;Error&quot;+e);
       }
	   finally{
		  lsLOVBC = null, lsLOVBO = null,searchExpr = null,isRecord = null;
       }
  }
"
